package ClassObject;
class Person{
	String name;
	String age;
	public Person(String name, String age) {
		super();
		this.name = name;
		this.age = age;
	}
	public void displayinfo() {
		System.out.println("Name : " +name);
		System.out.println("Age : " +age);
	}
	
}

public class ClassPro {

	public static void main(String[] args) {
		Person p=new Person("Laptop","20");
		p.displayinfo();
		

	}

}
